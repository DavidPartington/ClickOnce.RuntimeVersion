<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <Company>David Partington</Company>
    <Description>Decode the ClickOnce runtime version from the Package Manifest.  Only for  DotNet 5 or 6 </Description>
    <Copyright>David Partington</Copyright>
    <Authors>David Partington</Authors>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ClickOnce;Runtime;Version;DotNet5;DontNet6</PackageTags>
    <ProduceReferenceAssembly>True</ProduceReferenceAssembly>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <PackageProjectUrl>https://github.com/DavidPartington/ClickOnce.RuntimeVersion</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DavidPartington/ClickOnce.RuntimeVersion</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
